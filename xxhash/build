}

configure() {
  patch -p0 -i "$CERD"/$nom/patches/glaucus/0001-disable-static.patch
}

build() {
  make
}

check() {
  :
}

package() {
  make \
    DESTDIR="$SACD" \
    PREFIX=/usr \
    install
}
