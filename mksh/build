prepare() {
  mkdir -pv build
  cd build
}

configure() {
  :
}

build() {
  sh "$SRCD"/$nom/$nom/Build.sh \
    -r
}

check() {
  :
}

package() {
  mv -fv $nom "$SACD"/usr/bin
  ln -fnsv $nom "$SACD"/usr/bin/ksh
  ln -fnsv $nom "$SACD"/usr/bin/rksh
}
