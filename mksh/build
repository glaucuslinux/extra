build() {
  sh Build.sh -r
}

package() {
  mkdir -p "$dir"/usr/bin
  mv -f $nom "$dir"/usr/bin
  ln -fs $nom "$dir"/usr/bin/ksh
  ln -fs $nom "$dir"/usr/bin/rksh
}
