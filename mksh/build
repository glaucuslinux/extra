build() {
  sh Build.sh -r
}

package() {
  mkdir -pv "$SACD"/usr/bin
  mv -fv $nom "$SACD"/usr/bin
  ln -fnsv $nom "$SACD"/usr/bin/ksh
  ln -fnsv $nom "$SACD"/usr/bin/rksh
}
